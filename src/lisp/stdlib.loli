(define *lolisp-version* "0.2")
(defun quit () (exit 0))
(define pi 3.14159265)
(define *trait-char* 4)
(define *trait-lambda* 5)
(define #t 1)
(define #f (list))
(define *stdin* 0)
(define *stdout* 1)
(define *stderr* 2)
(defun string (in)
    (set-trait *trait-char* in))
(defun square (x) (* x x))
(defun get-char () (file-read *stdin* 1))
(defun y-or-n (str) (format "~s~%" str) (readline "#t/#f: "))

(format "Loaded!~%")
